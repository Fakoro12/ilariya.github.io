$('textarea[name="your_side_developers_dev"]',_this.$form),rules:[{'function':'not_empty','error_msg_field':'расскажите, кто будет принимать участие в разработке с вашей стороны',}],error_container_selector:'.js-container_selector',error_message_container:'.js-error_field_message',error_message_insert_method:'append'},budget_dev:{name:'Какой бюджет на дизайн и разработку сайта планируете выделить?',$:$('textarea[name="budget_dev"]',_this.$form),rules:[{'function':'not_empty','error_msg_field':'укажите бюджет',}],error_container_selector:'.js-container_selector',error_message_container:'.js-error_field_message',error_message_insert_method:'append'},};if(_this.debug)console.log('ValidateForms.field_templates.brief: end');var not_replace=['Укажите бюджет.'];return{'fields':fields,'error_set':{'enabel':!0,'selector_html':'.form-msg','filter':function(e){if(e.set.length>1){$.each(e.set,function(i,errorMsg){if(!not_replace.includes(errorMsg)){e.set[i]=errorMsg.replace(/^Укажите,? /,'');if(i!=e.set.length-1)e.set[i]=e.set[i].replace(/.$/,';')}else{e.set[i]=errorMsg.replace(/^Укажите,? /,'укажите ');if(i!=e.set.length-1)e.set[i]=e.set[i].replace(/.$/,';')}});return'<div class="form-msg-error">Пожалуйста, заполните все поля, укажите:'+e.before+e.set.join('</li><li>')+e.after+'</div>'}
return e.before+e.set.join('</li><li>')+e.after}}}}
ValidateForms.field_templates.subscribe=function(){var _this=this._this;var fields={name:{name:'name',$:$('input[name="name"]',_this.$form),rules:[{'function':'not_empty','error_msg_field':trnsl.t('Укажите ФИО','form')}],error_container_selector:'.inputbox',error_message_container:'.placeholder,.field-msg-container',error_message_insert_method:'append'},email:{name:'email',$:$('input[name="email"]',_this.$form),rules:['is_email'],error_container_selector:'.inputbox',error_message_container:'.placeholder,.field-msg-container',error_message_insert_method:'append'}};if(_this.debug)console.log('ValidateForms.field_templates.order: end');return fields}
