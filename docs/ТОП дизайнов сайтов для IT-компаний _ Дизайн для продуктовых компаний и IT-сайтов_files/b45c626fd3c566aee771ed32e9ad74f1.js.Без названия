function checkStorage(){$(formFields).each(function(index){$(this).on("change keyup keypress select",function(){if(this.type!=='submit'){if(this.type==='radio'){localStorage.setItem(this.name,this.value)}else{let repeatedInputsValues=[]
if(this.name.includes('[]')){let inputs=$("[name='"+this.name+"']")
for(let input of inputs){repeatedInputsValues.push($(input).val())}
localStorage.setItem(this.name,JSON.stringify(repeatedInputsValues))}else{localStorage.setItem(this.name,this.value)}}}})})}
checkStorage();function checkFields(){let formFields=[];$(".form-control").each(function(){formFields.push($(this))});return formFields}
function duplicateDo(elem,value){let $clone=''
let target=''
let search_class=''
let dynamicFieldsType1=['market_request[]','niche_request[]']
