function changeHandler(){let dynamicTableFields1=['market[]','niche[]','source[]']
let dynamicTableFields2=['client_description[]','client_market[]','client_niche[]']
let markets=JSON.parse(localStorage.getItem('market[]'))
let niches=JSON.parse(localStorage.getItem('niche[]'))
let sources=JSON.parse(localStorage.getItem('source[]'))
let rowsCount=markets!==null?markets.length:0;if(niches!==null){if(niches.length>rowsCount){rowsCount=niches.length}}
if(sources!==null){if(sources.length>rowsCount){rowsCount=sources.length}}
let table1RowClone=''
for(let i=0;i<rowsCount-1;i++){table1RowClone=$('.group-444:first').clone()
$(table1RowClone).find('.close-btn-td2').append('<div class="input-group-append input-group-append-table2"><button class="btn fa fa-close table-close-btn" type="button"></button></div>')
$('.add-group-444').append(table1RowClone)}
let table1Rows=$('.group-444')
$('.add-group-444').on('change keyup',function(){changeTableRowsStorage($(this))})
for(let i=0;i<=rowsCount;i++){if(markets!==null){$(table1Rows[i]).children().find("input[name='market[]']").val(markets[i])}
if(niches!==null){$(table1Rows[i]).children().find("input[name='niche[]']").val(niches[i])}
if(sources!==null){$(table1Rows[i]).children().find("input[name='source[]']").val(sources[i])}}
let client_descriptions=JSON.parse(localStorage.getItem('client_description[]'))
let client_markets=JSON.parse(localStorage.getItem('client_market[]'))
let client_niches=JSON.parse(localStorage.getItem('client_niche[]'))
let rowsCount2=client_descriptions!==null?client_descriptions.length:0;if(client_markets!==null){if(client_markets.length>rowsCount2){rowsCount2=client_markets.length}}
if(client_niches!==null){if(client_niches.length>rowsCount2){rowsCount2=client_niches.length}}
let table2RowClone=''
for(let i=0;i<rowsCount2-1;i++){table2RowClone=$('.group-666:first').clone()
$(table2RowClone).find('.close-btn-td2').append('<div class="input-group-append input-group-append-table2"><button class="btn fa fa-close table-close-btn" type="button"></button></div>')
$('.add-group-666').append(table2RowClone)}
let table2Rows=$('.group-666')
for(let i=0;i<=rowsCount2;i++){if(client_descriptions!==null){$(table2Rows[i]).children().find("input[name='client_description[]']").val(client_descriptions[i])}
if(client_markets!==null){$(table2Rows[i]).children().find("input[name='client_market[]']").val(client_markets[i])}
if(client_niches!==null){$(table2Rows[i]).children().find("input[name='client_niche[]']").val(client_niches[i])}}
function changeTableRowsStorage(table){let tds=$(table.children().not(':first')[0]).children().not(':last')
let keys=[]
for(let td of tds){keys.push($(td).find('input').attr('name'))}
for(let key of keys){localStorage.removeItem(key)}
let rows=table.children().not(':first')
let items=[]
for(let row of rows){for(let td of $(row).children().not(':last')){items.push({'key':$(td).find('input').attr('name'),'value':$(td).find('input').val()})}}
for(let key of keys){switch(key){case 'market[]':markets=[];break;case 'niche[]':niches=[];break;case 'source[]':sources=[];break;case 'client_description[]':client_descriptions=[];break;case 'client_market[]':client_markets=[];break;case 'client_niche[]':client_niches=[];break}}
for(let item of items){switch(item.key){case 'market[]':markets.push(item.value);break;case 'niche[]':niches.push(item.value);break;case 'source[]':sources.push(item.value);break;case 'client_description[]':client_descriptions.push(item.value);break;case 'client_market[]':client_markets.push(item.value);break;case 'client_niche[]':client_niches.push(item.value);break}}
localStorage.setItem('market[]',JSON.stringify(markets))
localStorage.setItem('niche[]',JSON.stringify(niches))
localStorage.setItem('source[]',JSON.stringify(sources))
localStorage.setItem('client_description[]',JSON.stringify(client_descriptions))
localStorage.setItem('client_market[]',JSON.stringify(client_markets))
localStorage.setItem('client_niche[]',JSON.stringify(client_niches))}
