$('input',$clone).val(value);$('input',$clone).attr('value',value);$('textarea',$clone).val(value);$('textarea',$clone).attr('value',value);$('.duplicate-show',$clone).show();$('.duplicate-hide',$clone).hide();$('.duplicate-add'+'.add-'+target).append($clone);if(dynamicFieldsType1.includes($(elem).attr('name'))){$($clone).find('.close-btn-td').append('<div class="input-group-append input-group-append-table"><button class="btn fa fa-close table-close-btn" type="button"></button></div>')}else{$($clone).find('.input-with-close-btn').append('<div class="input-group-append" style="height: 0;"><button class="btn fa fa-close close-btn" type="button"></button></div>')}
if($('input',$clone).val()&&$('input',$clone).prev().hasClass('placeholder')){$('input',$clone).prev().addClass('small')}else{$('input',$clone).prev().removeClass('small')}
