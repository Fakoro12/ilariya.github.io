e.data('countup','')}}
function getNumber(e){return trnsl.lng=='EN'?Number(e.text().replace(/,/g,'')):Number(e.text().replace(/\s/g,''))}
function setNumber(e,number){return trnsl.lng=='EN'?e.html(number.toString().replace(/(\d)(?=(\d{3})+\b)/g,'$1,')):e.html(number.toString().replace(/(\d)(?=(\d{3})+\b)/g,'$1&nbsp;'))}}});function halferImg(){$('.halfer:not(.homep_video-review) .halfer__img img').each(function(index){var $i=$(this),$p=$i.closest('.halfer__img');$i.css({'width':'100%','height':'auto','marginTop':0,'marginLeft':0,});if($i.height()<$p.height()){$i.css({'width':'auto','height':'100%',});var additiveLeft=$i.data('halfer-left');if(typeof(additiveLeft)==='undefined')additiveLeft=0;var marginLeft=($i.width()-$p.width())/2;if(Math.abs(additiveLeft)>marginLeft)additiveLeft=marginLeft;if(additiveLeft<1&&additiveLeft>-1)additiveLeft=0;$i.css({'marginLeft':-marginLeft+additiveLeft})}else{var additiveTop=$i.data('halfer-top');if(typeof(additiveTop)==='undefined')additiveTop=0;var marginTop=($i.height()-$p.height())/2;if(Math.abs(additiveTop)>marginTop)additiveTop=marginTop;if(additiveTop<1&&additiveTop>-1)additiveTop=0;$i.css({'marginTop':-marginTop+additiveTop})}});$('.videote-owl-carousel').trigger('refresh.owl.carousel')}
function q_word_end(n,sl_1,sl_2,sl_3){return(n%10==1&&n%100!=11?sl_1:(n%10>=2&&n%10<=4&&(n%100<10||n%100>=20)?sl_2:sl_3))}
function getCookie(name){let matches=document.cookie.match(new RegExp("(?:^|; )"+name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g,'\\$1')+"=([^;]*)"));return matches?decodeURIComponent(matches[1]):undefined}
