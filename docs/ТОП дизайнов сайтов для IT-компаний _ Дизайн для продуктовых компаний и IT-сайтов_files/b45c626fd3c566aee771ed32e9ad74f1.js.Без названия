let tds=$(table.children().not(':first')[0]).children().not(':last')
let keys=[]
for(let td of tds){keys.push($(td).find('input').attr('name'))}
for(let key of keys){localStorage.removeItem(key)}
$(this).parent().parent().parent().remove();let rows=table.children().not(':first')
let items=[]
for(let row of rows){for(let td of $(row).children().not(':last')){items.push({'key':$(td).find('input').attr('name'),'value':$(td).find('input').val()})}}
for(let key of keys){switch(key){case 'market[]':markets=[];break;case 'niche[]':niches=[];break;case 'source[]':sources=[];break;case 'client_description[]':client_descriptions=[];break;case 'client_market[]':client_markets=[];break;case 'client_niche[]':client_niches=[];break}}
for(let item of items){switch(item.key){case 'market[]':markets.push(item.value);break;case 'niche[]':niches.push(item.value);break;case 'source[]':sources.push(item.value);break;case 'client_description[]':client_descriptions.push(item.value);break;case 'client_market[]':client_markets.push(item.value);break;case 'client_niche[]':client_niches.push(item.value);break}}
localStorage.setItem('market[]',JSON.stringify(markets))
localStorage.setItem('niche[]',JSON.stringify(niches))
localStorage.setItem('source[]',JSON.stringify(sources))
localStorage.setItem('client_description[]',JSON.stringify(client_descriptions))
